{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "DevDocs Starter",
  "colors": {
    "primary": "#F300A8",
    "light": "#F300A8",
    "dark": "#F300A8"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "@DeDevsClub",
          "href": "https://x.com/DeDevsClub",
          "icon": "twitter"
        },
        {
          "anchor": "Community",
          "href": "https://whop.com/dedevs",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://blog.dedevs.club",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "https://dedevs.club/favicon.ico",
    "dark": "https://dedevs.club/favicon.ico"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:admin@dedevs.club"
      }
    ],
    "primary": {
      "type": "button",
      "label": "GitHub",
      "href": "https://github.com/DeDevsClub"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/DeDevsClub",
      "github": "https://github.com/DeDevsClub"
    }
  }
}